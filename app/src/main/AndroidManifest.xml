<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codewithtimzowen.contactlistapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContactListApp">
        <activity android:name=".MessageActivity"
            android:label="Message"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".NickNamesActivity"
            android:parentActivityName=".MainActivity"
            android:label="NickName"/>
        <activity android:name=".ProffesionsActivity"
            android:parentActivityName=".MainActivity"
            android:label="Profession"/>
        <activity android:name=".FamilyActivity"
            android:parentActivityName=".MainActivity"
            android:label="Family"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>

